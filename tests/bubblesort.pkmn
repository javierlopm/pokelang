--
    Implementacio'n de bubblesort en pokelang
--

funcball VOIDtorb nothing( pINTachu arr,pINTachu size):
    pINTachu ordenados;
vamo_a_calmano

funcball VOIDtorb bubble( pINTachu arr, 
                          pINTachu size ):      
    pINTachu temp;
    pINTachu ordenados;
    pINTachu pos;
    --pINTachu ordenados;--
    VOIDtorb *voidtype;
    pINTachu holis;
    vamo_a_lee(holis);
    --vamo_a_escribi(bubbles);--

    vamo_a_imprimi("hola mundo");

    temp = temp * temp;

    vamo_a_itera ordenados = 0 |size-2|1: 
        ordenados = 1;
        vamo_a_itera pos = ordenados |size-2|1:
            si arr[pos] < arr[pos+1]:
                temp       = arr[pos+1];
                arr[pos+1] = arr[pos];
                arr[pos]   = temp;
            vamo_a_calmano
        vamo_a_calmano
    vamo_a_calmano
vamo_a_calmano


