FORWARDtortle STRUCTabuzz pokeAux;

STRUCTabuzz pokeAux meeeeh;

funcball pINTachu fibo(pINTachu n):
    si n == 0 || n == 1:
        vamo_a_retorna 1;
    si_no:
        vamo_a_retorna fibo(n-1) + fibo(n-2);
    vamo_a_calmano
vamo_a_calmano

--
    Fibonacci iterativo
--
funcball pINTachu fibo_iterativo(pINTachu n):
    pINTachu n1;
    pINTachu n2;
    pINTachu iter;
    pINTachu aux;


    si n == 0:
        vamo_a_retorna 0;
    si_no:
        n2 = 0;
        n1 = 1;
        vamo_a_itera iter = 0|n|1: 
            aux = n1 + n2;
            n2  = n1;
            n1  = aux; 
        vamo_a_calmano

        vamo_a_retorna n1;
    vamo_a_calmano


vamo_a_calmano